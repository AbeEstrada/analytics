{
  "name": "analytics/client",
  "private": true,
  "version": "0.10.23",
  "license": "MIT",
  "amdName": "_analytics",
  "source": "../src/index.js",
  "main": "dist/client/analytics.js",
  "module": "dist/client/analytics.module.js",
  "unpkg": "dist/client/analytics.umd.js",
  "exports": {
		".": {
			"require": "./dist/client/analytics.js",
			"import": "./dist/client/analytics.module.js"
		}
	},
  "types": "./types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist ../dist/client ../dist/cdn",
    "prebuild": "npm run clean",
    "build": "npm run build:lib && npm run build:standalone",
    "build:lib": "microbundle --define NODE_ENV=production,BROWSER=true,SERVER=false,VERSION=1.1.1",
    "build:standalone": "microbundle build -o ./dist/cdn --define NODE_ENV=production,BROWSER=true,SERVER=false,VERSION=1.1.1 --external none -f iife,umd --no-compress",
    "gen-esm": "gen-esm-wrapper ./dist/client/analytics.js ./dist/client/index.mjs",
    "copy": "mkdirp ../dist && cp -rf dist/. ../dist",
    "postbuild": "npm run gen-esm && npm run copy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DavidWells/analytics"
  },
  "files": [
    "dist",
    "lib",
    "README.md"
  ],
  "typings": "lib/types.d.ts",
  "dependencies": {
    "@analytics/global-storage-utils": "^0.1.4",
    "@analytics/type-utils": "^0.5.4",
    "analytics-utils": "^1.0.9"
  },
  "ava": {
    "files": [
      "**/**/*.test.js"
    ],
    "require": [
      "esm",
      "@babel/register"
    ],
    "verbose": true,
    "failFast": true,
    "sources": [
      "**/*.{js,jsx}"
    ]
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "microbundle": "^0.14.2",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "sinon": "7.2.3"
  }
}
